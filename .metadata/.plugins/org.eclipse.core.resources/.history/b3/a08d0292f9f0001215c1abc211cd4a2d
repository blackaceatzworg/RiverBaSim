package riverbasim;

import repast.simphony.context.DefaultContext;

public class RiverBaSimContext extends DefaultContext<Object> {
	public PeopleContext() {
		super("PeopleContext"); // must match name in model.score
		GeographyParameters<Person> geoParams = new GeographyParameters<Person>();
		// These lines actually create our Geography.
		// Tell Repast to create a new geography projection
		// called "PeopleGeography" in this context.
		Geography<Person> peopleGeography = GeographyFactoryFinder
				.createGeographyFactory(null).createGeography(
						"PeopleGeography", this, geoParams);
		System.out.println("Created PeopleGeography");
	}
}
