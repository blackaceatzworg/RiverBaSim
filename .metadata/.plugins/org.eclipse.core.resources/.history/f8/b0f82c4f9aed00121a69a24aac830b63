package datamodel;

/**
 * @author Luis
 *
 */
public class Pollutant {

	private String name;
	private float concentration;
	
	public Pollutant(String name, float concentration){
		this.name = name;
		this.concentration = 0;
	}
	public float mergeWith(Pollutant other){
		return 0;
		
	}
}
